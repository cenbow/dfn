.sf_main6 ul .sf_pt_img2{ background:url(../images/pt.jpg) no-repeat -64px 0; width:66px; height:70px; display:block; margin-left:2px}
.sf_main6 ul .sf_pt_img3{ background:url(../images/pt.jpg) no-repeat -130px 0; width:83px; height:70px; display:block; margin-left:11px}
.sf_main6 ul .sf_pt_img4{ background:url(../images/pt.jpg) no-repeat -213px 0; width:115px; height:70px; display:block}
.sf_main6 ul .sf_pt_img5{ background:url(../images/pt.jpg) no-repeat -328px 0; width:121px; height:70px; display:block;}
.sf_main6 ul .sf_pt_img6{ background:url(../images/pt.jpg) no-repeat -449px 0; width:109px; height:70px; display:block}
.sf_bt{ width:1000px; margin:0 auto; margin-top:95px; padding-bottom:95px}
.sf_bt a{ background:url(../images/sf_zc_bt.jpg) no-repeat; width:200px; height:58px; display:block; margin-left:400px}
.sf_gotop{ width:1000px; margin:0 auto; }
.sf_gotop a{ background:url(../images/sf_go_top.jpg) no-repeat; width:55px; height:55px; display:block; position: fixed; bottom:20px; margin-left:1050px}